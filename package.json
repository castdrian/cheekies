{
  "name": "melon",
  "description": "the all-in-one k-pop discord bot",
  "module": "main.ts",
  "main": "src/main.ts",
  "type": "module",
  "version": "1.6.1",
  "scripts": {
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --fix --ext .ts src",
